{
  "name": "react-easy-stack",
  "version": "0.1.0-alpha.10",
  "description":
    "React state management with a minimal API. Made with ES6 Proxies.",
  "main": "dist/cjs.es5.js",
  "module": "dist/es.es5.js",
  "types": "types/index.d.ts",
  "files": ["dist", "types"],
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "test-builds": "node ./scripts/testBuilds.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint":
      "prettier '**/*.{js,jsx}' '!**/build/**' --ignore-path '.gitignore' --list-different",
    "lint-fix":
      "prettier '**/*.{js,jsx}' '!**/build/**' --ignore-path '.gitignore' --write",
    "install-examples": "COMMAND='npm i' node ./scripts/runOnExamples.js",
    "build-examples": "COMMAND='npm run build' node ./scripts/runOnExamples.js",
    "deploy-examples":
      "COMMAND='npm run deploy' node ./scripts/runOnExamples.js",
    "link-examples":
      "npm link && COMMAND='npm link react-easy-stack' node ./scripts/runOnExamples.js",
    "unlink-examples":
      "COMMAND='npm uninstall react-easy-stack && npm install react-easy-stack' node ./scripts/runOnExamples.js",
    "build": "node ./scripts/build.js",
    "build-toc": "node ./scripts/buildToc.js"
  },
  "author": {
    "name": "Miklos Bertalan",
    "email": "miklos.bertalan@risingstack.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:solkimicreb/react-easy-stack.git"
  },
  "bugs": {
    "url": "https://github.com/solkimicreb/react-easy-stack/issues"
  },
  "homepage": "https://github.com/solkimicreb/react-easy-stack#readme",
  "license": "MIT",
  "keywords": ["TODO"],
  "dependencies": {
    "@nx-js/observer-util": "^4.1.1",
    "@nx-js/queue-util": "^1.1.1",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/react": "^16.0.35",
    "babel-core": "6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "7.1.2",
    "babel-minify": "^0.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "buble": "^0.19.3",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "jest": "^22.3.0",
    "markdown-toc": "^1.1.0",
    "pre-push": "0.1.1",
    "prettier": "^1.6.1",
    "rollup": "^0.56.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-auto-external": "^1.0.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {},
  "pre-push": ["lint", "test-coverage"]
}
